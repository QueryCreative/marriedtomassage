!!!
%html{lang: "en"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title #{@title} | Married To Massage 
    %meta{name: "viewport", content: "width=device-width, initial-scale=1"}
    %meta{name: "copyright", content: "Married To Massage Â© 2015"}
    %meta{name: "description", content: "Welcome to Married To Massage, We're Committed to Your Health and Well-being"}
    %meta{name: "keywords", content: "Massage, Sacramento, Married To Massage, Sciatica Massage, TMJ Massage, Stone Massage, Health, Massage Therapy, Massage Therapy Sacramento"}
    %meta{name: "DC.title", content: "Married To Massage"}
    %meta{name: "robots", content:"index, follow"}

    %link{:href => "touchicon.png", :rel => "apple-touch-icon"}
    %link{:href => "favicon.png", :rel => "icon"}
      /[if IE] <link rel="shortcut icon" href="favicon.ico">
    %meta{:content => "#D83434", :name => "msapplication-TileColor"}
    %meta{:content => "tileicon.png", :name => "msapplication-TileImage"}

    %script{src: "//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"}
    %link{href: "//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css"}
    %script{src: "//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"}
    %script{src: "https://maps.googleapis.com/maps/api/js"}

    = stylesheet_link_tag    'application', media: 'all', |
      'data-turbolinks-track' => true                     |
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
    
    / Google analytics
    %script 
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-60432087-1', 'auto');
      ga('send', 'pageview');

    = render 'layouts/shim'

    / ShareThis
    /= render 'layouts/sharethishead'
    / Go to www.addthis.com/dashboard to customize your tools
    /%script{:async => "async", :src => "https://s7.addthis.com/js/300/addthis_widget.js#pubid=ra-55181f164231a203", :type => "text/javascript"} 

  %body#top
    .wrapper
      .header
        = render 'layouts/header'
      /.addthis_sharing_toolbox
      = yield
      = render 'layouts/gmaps'
      .push
    .footer
      = render 'layouts/footer'

    /ShareThis
    = render 'layouts/sharethisfoot'
    