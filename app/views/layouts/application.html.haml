!!!
%html{lang: "en"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title #{@title} | Netality Properties 
    %meta{name: "viewport", content: "width=device-width, initial-scale=1"}
    %meta{name: "copyright", content: "Netrality Properties Â© 2015"}
    %meta{name: "description", content: "Welcome to Netrality Where Stability is our Mission"}
    %meta{name: "keywords", content: "Netrality, Netrality Properties, IT, Property, Communications, Meet Me Room, Fiber, Industry"}
    %meta{name: "DC.title", content: "Netrality Properties"}
    %meta{name: "robots", content:"index, follow"}

    %script{src: "//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"}
    %link{href: "//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css"}
    %script{src: "//ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"}
    %script{src: "https://maps.googleapis.com/maps/api/js"}

    = stylesheet_link_tag    'application', media: 'all', |
      'data-turbolinks-track' => true                     |
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
    
    %script 
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-58610836-1', 'auto');
      ga('send', 'pageview');
    
    = render 'layouts/shim'

  %body
    = render 'layouts/header'
    = yield
    = render 'layouts/gmaps'
    = render 'layouts/footer'
    
/    = debug(params) if Rails.env.development?
